<template name="eabc3cf6">
  <view class="_div data-v-56c0841f theme-list">
    <view class="_div data-v-56c0841f list-item" wx:key="item.id" key="{{item.id}}" bindtap="handleProxy" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{json}}" wx:for-index="index" wx:for-item="item">
      <view class="_div data-v-56c0841f item-container">
        <view class="_div data-v-56c0841f item-image">
          <image src="{{item.thumbnail}}" mode="aspectFill" class="_img data-v-56c0841f"></image>
        </view>
      </view>
      <view class="_div data-v-56c0841f item-text">
        <view class="_div data-v-56c0841f item-title">{{item.name}}</view>
        <view class="_div data-v-56c0841f item-subTitle">{{item.description}}</view>
      </view>
    </view>
    <view class="_div data-v-56c0841f list-bottomLoad" wx:if="{{json.length > 0 && bottomLoading}}">
      <view class="_div data-v-56c0841f loading" wx:if="{{bottomLoading === 'loading'}}">加载中...</view>
      <view class="_div data-v-56c0841f nothing" wx:if="{{bottomLoading === 'nothing'}}">刷完了，休息一下吧</view>
      <view class="_div data-v-56c0841f error" wx:if="{{bottomLoading === 'error'}}">出错了，刷新试试</view>
    </view>
  </view>
</template>