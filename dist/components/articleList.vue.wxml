<template name="52cb8b9c">
  <view class="_div data-v-159a9e4c article-list">
    <view class="_div data-v-159a9e4c list-date" wx:if="{{date}}">{{date}}</view>
    <view class="_div data-v-159a9e4c list-item" wx:key="item.id" key="{{item.id}}" bindtap="handleProxy" data-eventid="{{'0-'+index}}" data-comkey="{{$k}}" wx:for="{{json}}" wx:for-index="index" wx:for-item="item">
      <view class="_div data-v-159a9e4c item-container images" wx:if="{{item.images}}">
        <view class="_div data-v-159a9e4c item-title">{{item.title}}</view>
        <view class="_div data-v-159a9e4c item-images">
          <image src="{{item.images[0] || item.images}}" mode="widthFix" class="_img data-v-159a9e4c"></image>
        </view>
      </view>
      <view class="_div data-v-159a9e4c item-container text" wx:else>
        <view class="_div data-v-159a9e4c item-title">{{item.title}}</view>
      </view>
    </view>
    <view class="_div data-v-159a9e4c list-bottomLoad" wx:if="{{json.length > 0 && bottomLoading}}">
      <view class="_div data-v-159a9e4c loading" wx:if="{{bottomLoading === 'loading'}}">加载中...</view>
      <view class="_div data-v-159a9e4c nothing" wx:if="{{bottomLoading === 'nothing'}}">刷完了，休息一下吧</view>
      <view class="_div data-v-159a9e4c error" wx:if="{{bottomLoading === 'error'}}">出错了，刷新试试</view>
    </view>
  </view>
</template>